  // * Title Section
  "rich-text#blog-title": {
    "props": {
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "text": "# revisa nuestro blog",
      "blockClass": ["Blog__title"]
    }
  },
  // * Slider Posts
  "flex-layout.row#blog-posts": {
    "props": {
      "blockClass": ["Blog__posts"]
    },
    "children": ["flex-layout.col#posts"]
  },
  "flex-layout.col#posts": {
    "props": {
      "blockClass": ["Blog__posts-container"]
    },
    "children": ["info-card#blog-post1", "info-card#blog-post2", "info-card#blog-post3"]
  },
