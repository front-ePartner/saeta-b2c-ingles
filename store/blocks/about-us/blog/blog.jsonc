{
  "flex-layout.row#container-info__blog": {
    "props": {
      "blockClass": ["Blog"]
    },
    "children": ["flex-layout.col#blog"]
  },
  "flex-layout.col#blog": {
    "props": {
      "blockClass": ["Blog__container"]
    },
    "children": ["rich-text#blog-title", "flex-layout.row#blog-posts"]
